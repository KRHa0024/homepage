{
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated", "anonymous"],
      "headers": {
        "Cache-Control": "no-cache"
      },
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/unauthorized.html",
      "statusCode": 401
    }
  }
}